{
  "name": "khalyomede/laravel-odata-adaptable",
  "description": "Adapt your Eloquent model automatically according to the OData query strings.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Khalyomede",
      "email": "khalyomede@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "khalyomede/odata-query-parser": "0.*"
  },
  "autoload": {
    "psr-4": {
      "Khalyomede\\": "src"
    }
  },
  "require-dev": {
    "laravel/framework": "5.8.*",
    "phpunit/phpunit": "8.*",
    "infection/infection": "0.*",
    "php-coveralls/php-coveralls": "2.*"
  },
  "scripts": {
    "test": "phpunit --coverage-clover build/logs/clover.xml test",
    "mutate": "infection"
  },
  "config": {
    "bin-dir": "bin"
  }
}
